{
    "name": "aogg/think-phpunit",
    "description": "ThinkPHP6 use PHPUnit.",
    "keywords": [
        "PHPUnit",
        "phpunit",
        "thinkphp"
    ],
    "homepage": "https://github.com/aogg/composer-PHPUnit-ThinkPHP/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "aogg",
            "email": "aogg@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0 || >=8.1",
        "ext-json": "*",
        "phpunit/phpunit": "^7.0 || ^10.5.17",
        "topthink/framework": "^6.0.0",
        "ext-pcntl": "*"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "aogg\\phpunit\\think\\": "src/",
            "aogg\\phpunit\\think\\testsDir": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "aogg\\phpunit\\think\\Service"
            ]
        }
    }
}
